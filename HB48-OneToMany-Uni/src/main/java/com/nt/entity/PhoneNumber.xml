<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
    <hibernate-mapping>
    <class name="com.nt.entity.PhoneNumber" table="PersonNumber_OTM">
    <id name="regNo" column="REGNO">
    <generator class="sequence">
    <param name="sequence_name">HB_OTM</param>
    <param name="initial_value">1000</param>
    <param name="increment_size">1</param>
    </generator>
    </id>
    <property name="mobileNo"/>
    <property name="numberType"/>
    <property name="provider"/>
    
            </class>
        </hibernate-mapping>