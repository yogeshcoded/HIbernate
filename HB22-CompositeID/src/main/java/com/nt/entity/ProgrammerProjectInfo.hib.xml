<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
	<hibernate-mapping>
	  <class name="com.nt.entity.ProgrammerProjectInfo"   >
	      <composite-id name="id" class="com.nt.entity.PrgmrProjId">
	           <key-property name="prgmrId"/>
	           <key-property name="projId"/>
	      </composite-id>
	     
	     <property name="prgmrName" length="20" />
	     <property name="salary" />
	     <property name="projName" length="20"/>
	     <property name="budget" />
	  </class>
	</hibernate-mapping>
